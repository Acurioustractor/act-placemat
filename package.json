{
  "name": "act-placemat",
  "version": "1.0.0",
  "description": "ACT Community Placemat with Notion Integration",
  "main": "src/server/server.js",
  "scripts": {
    "start": "node src/server/server.js",
    "dev": "concurrently \"nodemon src/server/server.js\" \"cd client && npm run dev\"",
    "dev:server": "nodemon src/server/server.js",
    "dev:client": "cd client && npm run dev",
    "dev:full": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:internal": "concurrently \"nodemon src/server/server.js\" \"cd client && npm run dev\"",
    "dev:public": "cd frontend-new && npm run dev",
    "dev:both": "concurrently \"npm run dev:internal\" \"npm run dev:public\"",
    "build": "cd client && npm run build",
    "test": "node src/testing/test-integrations.js",
    "test:integration": "node src/testing/test-integrations.js",
    "test:notion": "node src/testing/test-notion-integration.js",
    "test:client": "cd client && npm run test",
    "install-deps": "npm install && cd client && npm install",
    "lint": "cd client && npm run lint",
    "format": "cd client && npm run format"
  },
  "dependencies": {
    "@notionhq/client": "^4.0.1",
    "@supabase/supabase-js": "^2.52.0",
    "airtable": "^0.12.2",
    "cheerio": "^1.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "node-fetch": "^2.7.0",
    "openai": "^5.10.1",
    "puppeteer": "^24.14.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "community",
    "placemat",
    "notion",
    "act",
    "projects"
  ],
  "author": "ACT Community",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  }
}
