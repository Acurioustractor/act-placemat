{
  "dashboard": {
    "id": null,
    "title": "AI Services Monitoring",
    "tags": ["ai", "anthropic", "openai", "perplexity", "australia"],
    "timezone": "Australia/Sydney",
    "panels": [
      {
        "id": 1,
        "title": "AI Request Rate by Provider",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(ai_requests_total[5m])",
            "legendFormat": "{{provider}} {{operation}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "fillOpacity": 10,
              "stackSeries": "normal"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "AI Request Duration (95th Percentile)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(ai_request_duration_seconds_bucket[5m]))",
            "legendFormat": "{{provider}} {{model}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Token Usage by Provider",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(ai_tokens_used_total[5m])",
            "legendFormat": "{{provider}} {{model}} {{token_type}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "tokens/min",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "stackSeries": "normal"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "AI Costs (AUD)",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(ai_cost_usd_total) * 1.5",
            "legendFormat": "Total Cost (AUD)",
            "refId": "A"
          },
          {
            "expr": "sum(rate(ai_cost_usd_total[1h])) * 1.5 * 24",
            "legendFormat": "Daily Rate (AUD)",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyAUD",
            "decimals": 2
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "AI Request Success Rate by Provider",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(ai_requests_total{status=\"success\"}[5m]) / rate(ai_requests_total[5m]) * 100",
            "legendFormat": "{{provider}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 95
                },
                {
                  "color": "green",
                  "value": 99
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "AI Model Usage Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (model) (ai_requests_total)",
            "legendFormat": "{{model}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 7,
        "title": "Token Efficiency (Tokens per Request)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(ai_tokens_used_total[5m]) / rate(ai_requests_total[5m])",
            "legendFormat": "{{provider}} {{model}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "tokens",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 24
        }
      }
    ],
    "time": {
      "from": "now-3h",
      "to": "now"
    },
    "refresh": "30s",
    "schemaVersion": 36,
    "version": 1,
    "uid": "act-ai-services",
    "description": "Monitoring dashboard for AI service usage, costs, and performance in ACT Placemat"
  }
}