# Development Environment Configuration
NODE_ENV=development
PORT=3002
HOST=localhost
FRONTEND_URL=http://localhost:3000

# Redis for development (using default Redis instance)
REDIS_URL=redis://localhost:6379
REDIS_DB=1

# Logging
LOG_LEVEL=debug
ENABLE_METRICS=true

# Mock API keys for development (replace with real keys when available)
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
GOOGLE_API_KEY=

# Development security (not for production)
JWT_SECRET=development_jwt_secret_not_for_production_use_only
ENCRYPTION_KEY=dev_encryption_key_32_characters

# Development settings
DEBUG=intelligence-hub:*
ENABLE_AGENT_DEBUGGING=true
MOCK_AI_RESPONSES=true

# Rate limiting (more permissive for development)
RATE_LIMIT_MAX=1000
RATE_LIMIT_WINDOW=900000

# Australian settings
TIMEZONE=Australia/Sydney
LOCALE=en-AU
CURRENCY=AUD

# Community features
COMMUNITY_VOTING_ENABLED=true
DEMOCRATIC_AGENT_SELECTION=true
TRANSPARENCY_LOGGING=true